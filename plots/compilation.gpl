# set term qt;
set term pdf size 2.5in, 3.3in;
set output 'compilation.pdf';

unset xtics;
set nokey;
set ytics rotate;
set xtics rotate;

set boxwidth 0.5;
set style fill solid 0.5 border -1;
set xrange [0.5:4.5];
set yrange [0:2.6];
set bmargin 8;
unset colorbox;
set bar 2.5;

set palette model RGB defined ( 0 'red', 1 'green' )

set ylabel 'compilation time (s)'

plot \
    'compilation.txt' using 1:3:5:xtic(2) with boxes palette, \
    '' using 1:3:(3*$4) with errorbars lc 'black' pt 0;
